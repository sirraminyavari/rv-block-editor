{
    "compilerOptions": {
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "allowUmdGlobalAccess": true,
        "baseUrl": ".",
        "esModuleInterop": false,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "jsx": "react",
        "declaration": true,
        "outDir": "lib-dist",
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": true,
        "resolveJsonModule": true,
        "skipLibCheck": false,
        "strict": false,
        "target": "es2015",
        "types": [
            "node"
        ]
    },
    "include": [
        "BlockEditor/**/*"
    ],
    "exclude": [
        "node_modules",
        "vite.config.js"
    ]
}